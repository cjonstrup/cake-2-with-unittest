{
    "name": "cjonstrup/cake-2-with-unittest",
    "description": "The CakePHP framework with PHPUnitest 9.x",
    "license": "MIT",
    "require": {
        "php": "^7.3",
		"ext-mcrypt": "*",
        "cakephp/cakephp": "2.10.*"
    },
    "require-dev": {
		"phpunit/phpunit": "^9.3",
        "fzaninotto/faker": "^1.9"
    },
    "config": {
        "vendor-dir": "app/Vendor/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
		"test": "app/Vendor/bin/phpunit --configuration phpunit.xml --testsuite unittest --stderr --verbose"
	}
}
